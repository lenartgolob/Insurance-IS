@model IEnumerable<web.Models.InsuranceSubtype>

@{
    ViewData["Title"] = "InsuranceSubtypes";
}

<h1>InsuranceSubtypes</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Title
            </th>
            <th>
                Rate
            </th>
            <th>
                Insurance type
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InsuranceType.Title)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.InsuranceSubtypeID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.InsuranceSubtypeID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.InsuranceSubtypeID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>